{
  "name": "react-native-rxpicker",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "lint": "eslint app",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "node node_modules/jest/bin/jest.js --watch",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "clean": "rimraf app/package/dist",
    "lib-build": "npm run clean && babel app/package/src -d app/package/dist --source-maps",
    "clearFolder": "node ./script/web/rm-docs.js",
    "web-build": "set NODE_ENV=production && npm run clearFolder && webpack -p --config ./web/webpack.prod.config.js",
    "web-dev": "set NODE_ENV=development && webpack-dev-server -d --config ./web/webpack.dev.config.js --port 3001 --inline --hot --colors  --history-api-fallback",
    "web-devbuild": "set NODE_ENV=development && npm run clearFolder && webpack -p --config ./web/webpack.dev.config.js"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "react": "16.8.3",
    "react-art": "16.6.3",
    "react-dom": "16.6.3",
    "react-native": "0.59.10",
    "react-native-modal": "^11.5.6",
    "react-native-popup-dialog": "^0.18.3",
    "react-native-rxdialog": "^1.0.6",
    "react-native-web": "0.11.7"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/runtime": "^7.5.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "7.1.5",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-react-native-web": "0.11.7",
    "babel-plugin-react-transform": "3.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-native": "^4.0.1",
    "babel-preset-stage-0": "6.24.1",
    "css-loader": "^3.6.0",
    "eslint": "^7.5.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "jest": "^24.9.0",
    "metro-react-native-babel-preset": "0.59.0",
    "react-native-md5": "^1.0.0",
    "react-native-web-image-loader": "0.1.1",
    "react-test-renderer": "16.8.3",
    "rimraf": "^3.0.2",
    "style-loader": "^1.2.1",
    "url-loader": "^4.1.0",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.2.1"
  },
  "jest": {
    "preset": "react-native"
  }
}
